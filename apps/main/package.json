    "faker": "^4.1.0",
    "fs-extra": "^10.0.0",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^4.0.6",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^10.0.3",
    "patch-package": "^6.4.7",
    "pdf-lib": "^1.16.0",
    "postinstall-prepare": "^2.0.0",
    "prettier": "^2.7.1",
    "storybook": "^7.0.7",
    "typescript": "^4.4.3",
    "vite": "4.2.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "react-beautiful-dnd": "^13.0.0",
    "react-color": "^2.19.3",
    "react-csv": "^2.2.2",
    "react-date-range": "^1.0.3",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dom": "^18.2.0",
    "react-google-maps": "^9.4.5",
    "react-grid-layout": "^1.3.4",
    "react-hook-form": "^7.34.0",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=6144 DEBUG=vite:deps vite",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=6144 vite build",
    "build:main": "yarn build",
    "start:instance": "npx zx ../../scripts/local/start-instance.mjs",
    "start:m": "cross-env VITE_APP_ENABLE_INSTANCE_SWITCH=true yarn start",
    "dev": "concurrently \"yarn start\" \"yarn storybook\"",
    "serve": "vite preview",
    "dev:api": "vitest",
    "test:api": "vitest --silent",
    "test:ui": "vitest pull --rebase --autostash && amplify push api -y",
    "amp-push": "node check-amplify.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "dev-prepush": "amplify api gql- --ui",
    "coverage": "vitest run --coverage",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "npx eslint@8.40.0 -c .eslintrc-ide.json \"src/**/*.{js,jsx,ts,tsx}\"",
    "mock-schema": "node ../../scripts/appsync/introspec.js",
    "push-api": "gitcompile && cross-env ES_LAMBDA_ARN=arn:aws:lambda:ap-southeast-2:265681005590:function:mbBonsaiStreamFn-master node ../../scripts/proxy-es/pre-push",
    "dev-postpush": "cross-env ES_LAMBDA_ARN=arn:aws:lambda:ap-southeast-2:265681005590:function:mbBonsaiStreamFn-master node ../../scripts/proxy-es/post-push",
    "install-fn-deps": "yarn --cwd amplify/backend/function install",
    "test:e2e": "yarn install-fn-deps && yarn watch-e2e-test",
    "pull-schema": "yarn && node ../../scripts/cdk/pull-dev-schema.js",
    "pull-auth-groups": "yarn && node ../../scripts/cdk/pull-dev-schema.js",
    "codegen": "cd ../../scripts/codegen && yarn install && yarn graphql-codegen --config codegen.yml",
    "compile-cdk": "npx zx ../../scripts/cdk/index.mjs mbDev synth -q mbDev",
    "compile-cdk-lambda": "npx zx ../../scripts/cdk/index.mjs mbDev synth -q mbDev-lambda",
    "compile-cdk-hosting": "npx zx ../../scripts/cdk/index.mjs mbDev synth -q mbDev-hosting",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "cross-env NODE_OPTIONS=--max_old_space_size=6144 storybook build -s public",
    "test-storybook": "test-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
