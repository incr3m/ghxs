{
  "name": "xxx-v2",
  "description": "The Operating System for Services Business.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "version": "0.0.2",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "modules/core/*",
    "modules/instances/*"
  ],
  "scripts": {
    "dev:init": "cd apps/main && yarn install",
    "dev:build": "cd apps/main && yarn build",
    "start:instance": "turbo start:instance --",
    "start:m": "turbo start:m --",
    "start": "turbo start",
    "prepare": "husky install",
    "build": "turbo build --filter=./packages/*",
    "build:main": "turbo build --filter=./apps/main",
    "dev": "turbo dev",
    "build-storybook": "turbo build-storybook",
    "lint": "turbo lint",
    "lint-staged-husky": "lint-staged",
    "tsc": "turbo tsc",
    "test": "turbo test",
    "storybook": "turbo storybook",
    "storybook-build": "turbo storybook-build",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "commit": "git-cz",
    "clean": "npm exec --workspaces -- shx rm -rf node_modules .next .turbo coverage build dist compiled storybook-static && shx rm -rf node_modules",
    "remove-turbo-cache": "shx rm -rf ./node_modules/.cache/turbo",
    "update-dependencies": "turbo update-dependencies && npx npm-check-updates -u"
  },
  "dependencies": {
    "date-fns": "2.30.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "@mui/icons-material": "^5.10.16",
    "@mui/lab": "^5.0.0-alpha.110",
    "@mui/material": "^5.10.16",
    "@mui/styled-engine-sc": "^5.10.16",
    "@mui/styles": "^5.10.16",
    "@mui/x-data-grid": "^6.4.0",
    "@mui/x-data-grid-generator": "^6.4.0",
    "@mui/x-date-pickers": "^5.0.9",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@storybook/addons": "^7.0.7",
    "@storybook/addon-actions": "^7.0.7",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-jest": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-mdx-gfm": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@testing-library/dom": "9.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/bluebird": "^3.5.36",
    "@types/node": "20.1.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "commitizen": "4.3.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "shx": "0.3.4",
    "storybook": "7.0.9",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsup": "6.7.0",
    "turbo": "1.9.3",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=16.16.0",
    "npm": ">=9.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moreton-blue-software/embrace-v2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moreton-blue-software/embrace-v2/issues"
  },
  "homepage": "https://github.com/moreton-blue-software/embrace-v2#readme"
}
